{
  "name": "@dust/programs",
  "version": "0.0.0",
  "type": "module",
  "exports": {
    "./mud.config": "./dist/mud.config.js",
    "./out/*": "./dist/out/*.js"
  },
  "typesVersions": {
    "*": {
      "mud.config": ["./dist/mud.config.d.ts"],
      "out/*": ["./dist/out/*.d.ts"]
    }
  },
  "files": ["src", "out", "dist"],
  "scripts": {
    "build": "pnpm run build:mud && pnpm run build:js",
    "build:js": "tsc",
    "build:mud": "mud build && abi-ts --extension .ts",
    "clean": "forge clean && shx rm -rf src/**/codegen",
    "deploy:local": "pnpm run build && mud deploy",
    "fix": "pnpm run fix:format",
    "fix:format": "forge fmt",
    "fix:lint": "solhint --config ./.solhint.json '**/*.sol' --fix"
  },
  "dependencies": {
    "@dust/world": "workspace:*",
    "@latticexyz/config": "2.2.22-d621dc7c427a59eb74f4d379862646355a149338",
    "@latticexyz/schema-type": "2.2.22-d621dc7c427a59eb74f4d379862646355a149338",
    "@latticexyz/store": "2.2.22-d621dc7c427a59eb74f4d379862646355a149338",
    "@latticexyz/world": "2.2.22-d621dc7c427a59eb74f4d379862646355a149338",
    "@latticexyz/world-consumer": "2.2.22-d621dc7c427a59eb74f4d379862646355a149338",
    "@openzeppelin/contracts": "^4.9.6",
    "solady": "^0.1.12"
  },
  "devDependencies": {
    "forge-std": "https://github.com/foundry-rs/forge-std.git#3b20d60d14b343ee4f908cb8079495c07f5e8981",
    "solhint": "^3.4.1",
    "solhint-config-mud": "2.2.22-d621dc7c427a59eb74f4d379862646355a149338",
    "solhint-plugin-mud": "2.2.22-d621dc7c427a59eb74f4d379862646355a149338"
  }
}
