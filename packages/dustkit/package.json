{
  "name": "dustkit",
  "version": "0.0.0",
  "type": "module",
  "exports": {
    ".": "./dist/src/exports/index.js",
    "./internal": "./dist/src/exports/internal.js",
    "./mud.config": "./dist/mud.config.js",
    "./out/*": "./dist/out/*"
  },
  "typesVersions": {
    "*": {
      "index": ["./dist/src/exports/index.d.ts"],
      "internal": ["./dist/src/exports/internal.d.ts"],
      "mud.config": ["./dist/mud.config.d.ts"],
      "out/*": ["./dist/out/*.d.ts"]
    }
  },
  "files": ["dist", "json-schemas", "contracts", "out"],
  "scripts": {
    "build": "pnpm run build:json-schema && pnpm run build:mud && pnpm run build:js",
    "build:js": "tsc",
    "build:json-schema": "tsx scripts/generate-json-schemas.ts && biome check --write --config-path ../../biome.json json-schemas",
    "build:mud": "mud build && abi-ts --extension .ts",
    "test": "tsc --noEmit"
  },
  "dependencies": {
    "@dust/world": "workspace:*",
    "@latticexyz/abi-ts": "2.2.21-b18c0ef0edeab2378b08d9f4a328a5d0d817f6bf",
    "@latticexyz/cli": "2.2.21-b18c0ef0edeab2378b08d9f4a328a5d0d817f6bf",
    "@latticexyz/schema-type": "2.2.21-b18c0ef0edeab2378b08d9f4a328a5d0d817f6bf",
    "@latticexyz/store": "2.2.21-b18c0ef0edeab2378b08d9f4a328a5d0d817f6bf",
    "@latticexyz/world": "2.2.21-b18c0ef0edeab2378b08d9f4a328a5d0d817f6bf",
    "arktype": "^2.1.15",
    "ox": "^0.6.10"
  },
  "devDependencies": {
    "viem": "2.23.2"
  },
  "peerDependencies": {
    "viem": "2.x"
  }
}
