{
  "name": "dustkit",
  "version": "0.0.0",
  "type": "module",
  "exports": {
    ".": "./dist/src/exports/index.js",
    "./internal": "./dist/src/exports/internal.js",
    "./mud.config": "./dist/mud.config.js",
    "./out/*": "./dist/out/*.js"
  },
  "typesVersions": {
    "*": {
      "index": ["./dist/src/exports/index.d.ts"],
      "internal": ["./dist/src/exports/internal.d.ts"],
      "mud.config": ["./dist/mud.config.d.ts"],
      "out/*": ["./dist/out/*.d.ts"]
    }
  },
  "files": ["dist", "json-schemas", "contracts", "out"],
  "scripts": {
    "build": "pnpm run build:json-schema && pnpm run build:mud && pnpm run build:js",
    "build:js": "tsc",
    "build:json-schema": "tsx scripts/generate-json-schemas.ts && biome check --write --config-path ../../biome.json json-schemas",
    "build:mud": "mud build && abi-ts --extension .ts",
    "dev": "pnpm run build && tsc --watch",
    "test": "tsc --noEmit"
  },
  "dependencies": {
    "@dust/world": "workspace:*",
    "@latticexyz/abi-ts": "2.2.24-dc349e170742ab243c779d9a73a6bfbcc394b707",
    "@latticexyz/cli": "2.2.24-dc349e170742ab243c779d9a73a6bfbcc394b707",
    "@latticexyz/common": "2.2.24-dc349e170742ab243c779d9a73a6bfbcc394b707",
    "@latticexyz/schema-type": "2.2.24-dc349e170742ab243c779d9a73a6bfbcc394b707",
    "@latticexyz/store": "2.2.24-dc349e170742ab243c779d9a73a6bfbcc394b707",
    "@latticexyz/world": "2.2.24-dc349e170742ab243c779d9a73a6bfbcc394b707",
    "@latticexyz/world-module-metadata": "2.2.24-dc349e170742ab243c779d9a73a6bfbcc394b707",
    "arktype": "^2.1.15",
    "debug": "^4.4.1",
    "ox": "^0.6.10",
    "p-retry": "^6.2.1"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "viem": "2.30.6"
  },
  "peerDependencies": {
    "viem": "2.x"
  }
}
